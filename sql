 CREATE TABLE type_transaction(
 id BIGSERIAL PRIMARY KEY,
 name_transaction VARCHAR (80) NOT NULL
 );

 INSERT INTO type_transaction (name_transaction)
 VALUES('DEBIT'),('CREDIT'), ('TRANSFER');


 CREATE TABLE transactions (
 id BIGSERIAL PRIMARY KEY,
 value NUMERIC NOT NULL,
 currency VARCHAR (20),
 date TIMESTAMP,
 id_account BIGSERIAL,
 accountSender_id BIGSERIAL,
 accountRecipient_id BIGSERIAL,
 id_type BIGSERIAL
 );