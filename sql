 CREATE TABLE accounts (
 id BIGSERIAL PRIMARY KEY,
 iban VARCHAR (210) UNIQUE NOT NULL,
 balance NUMERIC NOT NULL,
 createdAt TIMESTAMP,
 id_bank BIGSERIAL,
 id_user BIGSERIAL
 );

